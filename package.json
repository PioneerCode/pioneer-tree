{
  "name": "pioneer-tree",
  "version": "2.0.3",
  "description": "Angular tree component.",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=production",
    "start:dev": "ng serve pioneer-tree-dev",
    "build": "ng build",
    "build:demo": "ng build pioneer-tree --configuration=production",
    "build:demo:404": "node ./build/404.js",
    "build:demo:prod": "npm run build:demo && npm run build:demo:404",
    "build:lib": "npm run build:lib:package && npm run build:lib:sass && npm run build:lib:theme  && npm run build:lib:readme",
    "build:lib:publish": "npm run build:lib && npm publish dist/pctree --access public",
    "build:lib:publish:tag": "npm run build:lib && npm publish dist/pctree --access public --tag rc",
    "build:lib:package": "ng build pctree",
    "build:lib:readme": "node ./build/readme.js",
    "build:lib:theme": "node ./build/theme.js",
    "build:lib:sass": "node ./build/sass.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.6",
    "@angular/common": "^6.1.6",
    "@angular/compiler": "^6.1.6",
    "@angular/core": "^6.1.6",
    "@angular/forms": "^6.1.6",
    "@angular/http": "^6.1.6",
    "@angular/platform-browser": "^6.1.6",
    "@angular/platform-browser-dynamic": "^6.1.6",
    "@angular/router": "^6.1.6",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular-devkit/build-ng-packagr": "~0.6.3",
    "@angular/cli": "~6.1.5",
    "@angular/compiler-cli": "^6.1.6",
    "@angular/language-service": "^6.1.6",
    "@pioneer-code/pioneer-tree": "2.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "bootstrap": "^4.1.1",
    "chalk": "^2.4.1",
    "codelyzer": "~4.2.1",
    "copyfiles": "^2.0.0",
    "cpy": "^7.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.9.2"
  }
}
